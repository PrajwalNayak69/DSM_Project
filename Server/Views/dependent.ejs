<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dependent</title>
</head>
<body>
    <table style="border: 2px solid">
        <tr>
            <th>essn</th>
            <th>name</th>
        </tr>
 <% data.forEach(function(item, index){ %>
    <tr>
        <td style="border: 2px solid; padding: 2px;"><%= item.essn %></td>
        <td style="border: 2px solid;  padding: 2px;"><%= item.name %></td>
        <td style="border: 2px solid;  padding: 2px;"><%= item.sex %></td>
        <td style="border: 2px solid;  padding: 2px;"><%= item.bdate %></td>
        <td style="border: 2px solid;  padding: 2px;"><%= item.bdate %></td>

      
    </tr>
    <% }); %>
</table>
</body>
</html>